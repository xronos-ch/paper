@phdthesis{Alcantara2021,
  title = {Entre Tepalcates. {{Hacia}} Una Comprensi{\'o}n Historiogr{\'a}fica de La Dataci{\'o}n Arqueom{\'e}trica En La Arqueolog{\'i}a Mexicana (1950-2020)},
  author = {Alc{\'a}ntara, Alberto},
  year = {2021},
  month = jul,
  doi = {10.13140/RG.2.2.24447.74400/2},
  abstract = {En esta tesis se hace una recopilaci{\'o}n de los trabajos de dataci{\'o}n arqueom{\'e}trica a lo largo del periodo 1950 a 2020 y sus aplicaciones en sitios arqueol{\'o}gicos mexicanos. A partir de dicha recopilaci{\'o}n se realiza un an{\'a}lisis historiogr{\'a}fico para comprender las relaciones acad{\'e}micas que se han formado entre arque{\'o}logos y arque{\'o}metras a lo largo de la segunda mitad del siglo XX. This thesis is a compilation of archaeometric dating works from 1950 to 2020 and their applications in Mexican archaeological sites. From this compilation, a historiographical analysis is carried out to understand the academic relationships that have been formed between archaeologists and archaeometrists throughout the second half of the 20th century.}
}

@article{AndersonEtAl2010,
  title = {{{PIDBA}} ({{Paleoindian Database}} of the {{Americas}}) 2010: {{Current Status}} and {{Findings}}},
  shorttitle = {Pidba (Paleoindian {{Database}} of the {{Americas}}) 2010},
  author = {Anderson, David G. and Miller, D. Shane and Yerka, Stephen J. and Gillam, J. Christopher and Johanson, Erik N. and Anderson, Derek T. and Goodyear, Albert C. and Smallwood, Ashley M.},
  year = {2010},
  journal = {Archaeology of Eastern North America},
  volume = {38},
  eprint = {40914542},
  eprinttype = {jstor},
  pages = {63--89},
  publisher = {Eastern States Archeological Federation},
  issn = {0360-1021},
  urldate = {2024-08-03},
  abstract = {The compilation and dissemination of primary data from multiple sources and across large areas is one of the major challenges facing the archaeological profession in the twenty-first century. The Paleoindian Database of the Americas (PIDBA), available on-line at http://pidba.utk.edu, integrates database and GIS technology to make available locational data on nearly 30,000 projectile points, attribute data on over 15,000 artifacts, and image data on ca. 7,500 points from across North America. These data can be used to explore patterns of land and lithic raw material use and demographic trends within the Paleoindian period. Equally important, PIDBA serves as a model for collaborative interaction between professional and avocational archaeological communities across the Americas. PIDBA grows through the voluntary collection and compilation of primary data, and the website provides instructions on how to record and submit this information. Recent additions to PIDBA include radiometrie and bibliographic databases, color artifact photographs, and artifact distribution maps that include accurate glacial, periglacial and pluvial lake, and shoreline/sea-level boundaries. Ongoing activity is directed to finding and adding in new information and compiling the attribute data from multiple sources into a single comprehensive database. All those interested in participating in the project are welcome and encouraged to do so.}
}

