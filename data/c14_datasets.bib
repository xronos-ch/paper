@article{DouglassEtAl2019,
  title = {A Critical Review of Radiocarbon Dates Clarifies the Human Settlement of {{Madagascar}}},
  author = {Douglass, Kristina and Hixon, Sean and Wright, Henry T and Godfrey, Laurie R and Crowley, Brooke E and Manjakahery, Barth{\'e}l{\'e}my and Rasolondrainy, Tanambelo and Crossland, Zo{\"e} and Radimilahy, Chantal},
  year = {2019},
  month = oct,
  journal = {Quat. Sci. Rev.},
  volume = {221},
  pages = {105878},
  issn = {0277-3791},
  doi = {10.1016/j.quascirev.2019.105878},
  abstract = {The timing of the human settlement of Madagascar, one of the last large landmasses to be settled by people, remains a key topic of debate in archaeology. Despite decades of research, recent estimates for initial settlement are increasingly divergent and span ca. 9000 years: the widest colonization window for any island within the reliable range of radiocarbon (14C) dating. 14C dating of archaeological sites and remains of butchered animals provide important evidence of when the island was first settled, but the reliability of these dates requires critical evaluation. Applying principles of chronometric hygiene, we present the first systematic review of Madagascar's14C chronology to clarify the island's settlement. Our findings support human presence by at least 2000 cal BP and suggest that an Early Holocene arrival is possible. The nature of such an early presence on the island, however, remains elusive due to a lack of contextual information.}
}

@article{FlohrEtAl2016,
  title = {Evidence of Resilience to Past Climate Change in {{Southwest Asia}}: {{Early}} Farming Communities and the 9.2 and 8.2 Ka Events},
